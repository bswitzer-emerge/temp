{
  "sections": {
    "main": {
      "type": "main-product-module",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": false
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "collapsible-row-1": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Testing Results",
            "icon": "box",
            "content": "<p><strong>Battery<\/strong>: {{ product.metafields.custom.battery_type.value }}<\/p><p><strong>Capacity New:<\/strong> {{ product.metafields.custom.capacity_new.value }}<\/p><p><strong>State of health<\/strong>: {{ product.metafields.custom.soh.value }}%<\/p><p><strong>Model<\/strong>: {{ product.metafields.custom.make.value }} {{ product.metafields.custom.model.value }}<\/p><p><strong>Type<\/strong>: {{ product.metafields.custom.modules_cells.value }}<\/p><p><strong>Reason for Retirement: <\/strong>{{ product.metafields.custom.reason_for_retirement.value }}<\/p><p><strong>Vendor<\/strong>: {{ product.vendor }}<\/p><p><\/p>",
            "page": ""
          }
        },
        "collapsible-row-0": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Storage condictions",
            "icon": "recycle",
            "content": "<p><\/p><p><\/p>",
            "page": "specs-sheet"
          }
        }
      },
      "block_order": [
        "title",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "collapsible-row-1",
        "collapsible-row-0"
      ],
      "settings": {
        "enable_sticky_info": true,
        "media_position": "left",
        "gallery_layout": "columns",
        "media_size": "medium",
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "disabled": true,
      "settings": {
        "heading": "",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "background-1",
        "image_ratio": "square",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 28
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
