        "69373153-7750-41a8-a55b-1f9e0e7f1623": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<style>\n    #power {\n        margin-top: 3px;\n        margin-right: 4px;\n    }\n    #hr2 {\n       margin-bottom: 10px;\n    }\n    #info-soh, #info-item {\n        position: relative;\n        top: 3px;\n        left: 4px;\n        display: inline-block;\n    }\n    #add-to-cart-circle {\n        display: inline-block;\n        border-radius: 50%;\n        width: 20px;\n        height: 20px;\n        background-color: green;\n        margin-right: 5px;\n    }\n    #add-to-cart-circle.med-green {\n        background-color: #C8E400;\n    }\n    #add-to-cart-circle.orange {\n        background-color: orange;\n    }\n    #add-to-cart-circle.yellow {\n        background-color: yellow;\n    }\n    #add-to-cart-circle.red {\n        background-color: red;\n    }\n <\/style>\n\n<div class=\"row\">\n    <div class=\"col-1\">\n        Type\n    <\/div>\n    <div class=\"col-2\">\n        {{ product.metafields.custom.gen_type }}\n    <\/div>\n<\/div>\n<div class=\"row\">\n    <div class=\"col-1\">\n        Capacity\n    <\/div>\n    <div class=\"col-2\">\n        <svg height=\"17\" id=\"power\" viewBox=\"0 0 10.7658321 17\" width=\"10.765832\" xmlns=\"http:\/\/www.w3.org\/2000\/svg\"><path d=\"m1106.66018 882.209607 1.00147-6.860088h-4.03092l7.03534-10.139912-.97643 7.736377h4.70692z\" transform=\"translate(-1103.630725 -865.209607)\"\/><\/svg> \n        {{ product.metafields.custom.capacity_new }}\n    <\/div>\n<\/div>\n<div class=\"row\">\n    <div class=\"col-1\">\n        Year Range\n    <\/div>\n    <div class=\"col-2\">\n        {{ product.metafields.custom.year_range }}\n    <\/div>\n<\/div>\n<div class=\"row\">\n    <div class=\"col-1\">\n        State of Health <div class=\"info\" id=\"info-soh\" data-tooltip=\"text goes here\" data-flow=\"top\">\n            <svg height=\"18\" viewBox=\"0 0 18 18\" width=\"18\" xmlns=\"http:\/\/www.w3.org\/2000\/svg\"><path d=\"m103.636647 711.23666c-4.9627995 0-9.0000005 4.037201-9.0000005 9s4.037201 9 9.0000005 9c4.962799 0 9-4.037201 9-9s-4.037201-9-9-9zm0 3.750046c.414047 0 .749954.335907.749954.749954s-.335907.749954-.749954.749954c-.414048 0-.749955-.335907-.749955-.749954s.335907-.749954.749955-.749954zm1.6875 10.499908h-3.375c-.414734 0-.749955-.335907-.749955-.749954s.335221-.749954.749955-.749954h.937545v-4.5h-.5625c-.414734 0-.750091-.336044-.750091-.750092 0-.41391.335357-.749954.750091-.749954h1.312455c.414733 0 .749954.336044.749954.749954v5.250092h.937546c.414733 0 .749954.335907.749954.749954s-.335221.749954-.749954.749954z\" fill=\"#a7a4a1\" transform=\"translate(-94.636647 -711.23666)\"><\/path><\/svg>\n        <\/div>\n    <\/div>\n    <div class=\"col-2\">\n        <span id=\"add-to-cart-circle\"><\/span>\n        {{ product.metafields.custom.soh }}%\n    <\/div>\n<\/div>\n<div class=\"row\">\n    <div class=\"col-1\">\n        Item ID <div class=\"info\" id=\"info-item\" data-tooltip=\"text goes here\" data-flow=\"top\">\n            <svg height=\"18\" viewBox=\"0 0 18 18\" width=\"18\" xmlns=\"http:\/\/www.w3.org\/2000\/svg\"><path d=\"m103.636647 711.23666c-4.9627995 0-9.0000005 4.037201-9.0000005 9s4.037201 9 9.0000005 9c4.962799 0 9-4.037201 9-9s-4.037201-9-9-9zm0 3.750046c.414047 0 .749954.335907.749954.749954s-.335907.749954-.749954.749954c-.414048 0-.749955-.335907-.749955-.749954s.335907-.749954.749955-.749954zm1.6875 10.499908h-3.375c-.414734 0-.749955-.335907-.749955-.749954s.335221-.749954.749955-.749954h.937545v-4.5h-.5625c-.414734 0-.750091-.336044-.750091-.750092 0-.41391.335357-.749954.750091-.749954h1.312455c.414733 0 .749954.336044.749954.749954v5.250092h.937546c.414733 0 .749954.335907.749954.749954s-.335221.749954-.749954.749954z\" fill=\"#a7a4a1\" transform=\"translate(-94.636647 -711.23666)\"><\/path><\/svg>\n        <\/div>\n    <\/div>\n    <div class=\"col-2\">\n        {{ product.metafields.custom.item_id }}\n    <\/div>\n<\/div>\n\n<hr id=\"hr2\">\n\n  <script>\n       function simpleTestCart() {\n            const stateOfHealthCircle = document.getElementById(\"add-to-cart-circle\");\n\n            const stateOfHealth = {{ product.metafields.custom.soh }};\n            let myClass = \"green\"\n            if (stateOfHealth >= 80 ) {\n                myClass = \"green\"\n            } else if (stateOfHealth <= 80 && stateOfHealth >= 60 ) {\n                myClass = \"med-green\"\n            } else if (stateOfHealth <= 60 && stateOfHealth >= 40 ) {\n                myClass = \"yellow\"\n            } else if (stateOfHealth <= 40 && stateOfHealth >= 20 ) {\n                myClass = \"orange\"\n            } else if (stateOfHealth <= 20 ) {\n                myClass = \"red\"\n            }\n\n            stateOfHealthCircle.classList.add(myClass);\n        }\n        simpleTestCart();\n    <\/script>"
          }
        },
