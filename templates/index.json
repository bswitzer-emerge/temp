    "d43bafa2-8f8f-40f6-a5dc-daed56a7a39f": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<style>\n  #fourup img {\n    max-width: 100%;\n    max-height: 144px;\n    object-fit: contain;\n  }\n  #fourup li p, #fourup li {\n    text-align: center;\n    font-family: \"Roboto Mono\", monospace;\n  }\n  #fourup li {\n    padding-left: 40px;\n    padding-right: 40px;\n    display:  flex;\n    flex-direction: column;\n    justify-content: space-between;\n    align-items: center;\n    position: relative;\n    \n  }\n  #fourup li:after {\n    position: absolute;\n    top: 31px;\n    right: -21px;\n    content: \"\";\n    background-image: url(\"https:\/\/cdn.shopify.com\/s\/files\/1\/0719\/5269\/5597\/files\/arrow-right-sm.svg?v=1680125743\");\n    background-repeat: no-repeat;\n    background-position: center;\n    width: 39px;\n    height: 100%;\n    \n  }\n  #fourup li:last-of-type::after {\n    display: none;\n    background-image: none;\n  }\n  #reposition1 {\n    position: relative;\n    top: 12px;\n  }\n  #reposition2 {\n    position: relative;\n    top: 23px;\n  }\n<\/style>\n<div class=\"bg\">\n<div class=\"page-width\" id=\"fourup\">\n    <ul class=\"multicolumn-list contains-content-container grid grid--1-col-tablet-down grid--4-col-desktop\">\n      <li class=\"multicolumn-list__item grid__item\">\n          <p>car<\/p>\n          <img src=\"\/\/cdn.shopify.com\/s\/files\/1\/0719\/5269\/5597\/files\/car_10c852e8-4ca0-4c79-883f-f55a5272913c.png?v=1678492591&amp;width=1420\" alt=\"\" class=\"multicolumn-card__image laziness\">\n      <\/li>\n      <li class=\"multicolumn-list__item grid__item\">\n       <p>warehouse<\/p>\n        <img src=\"\/\/cdn.shopify.com\/s\/files\/1\/0719\/5269\/5597\/files\/warehouse.png?v=1678492628&amp;width=1420\" alt=\"\"  class=\"multicolumn-card__image  laziness\">\n      <\/li>\n      <li class=\"multicolumn-list__item grid__item\">\n        <p>platform<\/p>\n        <img src=\"\/\/cdn.shopify.com\/s\/files\/1\/0719\/5269\/5597\/files\/platform.png?v=1678492644&amp;width=1420\" alt=\"\" class=\"multicolumn-card__image laziness\" id=\"reposition1\">\n      <\/li>\n      <li class=\"multicolumn-list__item grid__item\">\n            <p>buyer<\/p>\n            <img src=\"\/\/cdn.shopify.com\/s\/files\/1\/0719\/5269\/5597\/files\/dude.png?v=1678492672&amp;width=1420\" alt=\"\" class=\"multicolumn-card__image laziness\" id=\"reposition2\">\n      <\/li>\n    <\/ul>\n<\/div>\n<\/div>\n<style>\n.laziness {\n  opacity: 0;\n  transition: all 2s;\n  transform: translateY(15px);\n}\n.laziness.loaded {\n  opacity: 1;\n  transform: translateY(0px);\n}\n<\/style>\n\n\n<script>\nconst images = document.querySelectorAll('.laziness');\n\nconst options = {\n  rootMargin: '0px',\n  threshold: 0.95\n};\n\nconst observer = new IntersectionObserver(function(entries, observer) {\n  entries.forEach(entry => {\n    if (entry.isIntersecting) {\n      const img = entry.target;\n      \/\/img.setAttribute('src', img.dataset.src);\n      img.classList.add('loaded');\n      observer.unobserve(img);\n    }\n  });\n}, options);\n\nimages.forEach(image => {\n  observer.observe(image);\n});\n\n\n<\/script>",
